{
  "name": "recast.ly",
  "version": "1.1.0",
  "description": "Making YouTube with React and ES6",
  "author": "Hack Reactor <curriculum.team@hackreactor.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel . --watch --out-dir compiled --presets=react --ignore=node_modules,compiled --source-maps inline",
    "test": "live-server --open=./SpecRunner.html --port=8081",
    "start": "live-server --open=./index.html --port=8082",
    "postinstall": "curl -s https://raw.githubusercontent.com/reactorcore/pomander/master/bin/install | bash"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "jquery": "^2.2.1",
    "live-server": "^1.2.1",
    "lodash": "^4.17.21",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-react": "1.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "2.3.3",
    "sinon": "1.17.3"
  },
  "babel": {
    "presets": [
      "react"
    ]
  }
}
